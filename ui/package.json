{
	"name": "ui",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.11"
	},
	"scripts": {
		"dev": "rm -rf .svelte-kit && svelte-kit sync && vite dev --port 3001",
		"build": "vite build",
		"preview": "vite preview --port 3001",
		"format": "prettier '**/*.{svelte,js,ts,mjs,json}' --write",
		"test": "vitest --run",
		"graphqlgen": "graphql-codegen --config graphql.codegen.ts"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@iconify/svelte": "^4.0.2",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/node": "^20.12.11",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.16",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"@tanstack/svelte-query": "^5.35.5",
		"@urql/svelte": "^4.2.0"
	},
	"prettier": {
		"printWidth": 120,
		"tabWidth": 2,
		"semi": false,
		"singleQuote": true,
		"endOfLine": "lf",
		"trailingComma": "es5",
		"plugins": [
			"prettier-plugin-svelte"
		],
		"useTabs": true,
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	}
}
